#. module: club
#: model:mail.template,body_html:club.email_template_membership_affiliation_invitation
msgid ""
"<div summary=\"o_mail_template\" style=\"padding:0px;width:600px;margin:auto;background: #FFFFFF repeat top /100%;color:#777777\">\n"
"    <p>\n"
"        Dear <t t-out=\"object.member_id.name.split()[0] or 'member'\">member</t>,\n"
"    </p>\n"
"    <p>\n"
"        An affiliation request has been done for the season <u><i t-out=\"object.period_category_id.period_id.name or ''\">xxxx</i></u>.\n"
"        <br/>\n"
"        Could you please let us know your answer by clicking on the button of your choice?\n"
"    </p>\n"
"    <p>\n"
"        <a t-attf-href=\"/club/my/membership/accept?token={{ object.token }}\" style=\"padding: 5px 10px; font-size: 12px; line-height: 18px; color: #FFFFFF; border-color:#875A7B; text-decoration: none; display: inline-block; margin-bottom: 0px; font-weight: 400; text-align: center; vertical-align: middle; cursor: pointer; white-space: nowrap; background-image: none; background-color: #875A7B; border: 1px solid #875A7B; border-radius:3px\">Accept</a>\n"
"        <a t-attf-href=\"/club/my/membership/decline?token={{ object.token }}\" style=\"padding: 5px 10px; font-size: 12px; line-height: 18px; color: #FFFFFF; border-color:#875A7B; text-decoration: none; display: inline-block; margin-bottom: 0px; font-weight: 400; text-align: center; vertical-align: middle; cursor: pointer; white-space: nowrap; background-image: none; background-color: #875A7B; border: 1px solid #875A7B; border-radius:3px\">Decline</a>\n"
"        <a t-attf-href=\"/club/my/membership/view?token={{ object.token }}\" style=\"padding: 5px 10px; font-size: 12px; line-height: 18px; color: #FFFFFF; border-color:#875A7B; text-decoration: none; display: inline-block; margin-bottom: 0px; font-weight: 400; text-align: center; vertical-align: middle; cursor: pointer; white-space: nowrap; background-image: none; background-color: #875A7B; border: 1px solid #875A7B; border-radius:3px\">View</a>\n"
"    </p>\n"
"    <p>\n"
"        Best regards,\n"
"    </p>\n"
"    <p>\n"
"        The <t t-out=\"user.company_id.name or ''\">YourCompany</t> committee members\n"
"    </p>\n"
"    <img src=\"/logo.png\" style=\"padding: 0px; margin: 0px; height: auto; width: 80px;\" alt=\"{{ user.company_id.name }}\"/>\n"
"\n"
"</div>\n"
"        "
msgstr ""
"<div summary=\"o_mail_template\" style=\"padding:0px;width:600px;margin:auto;background: #FFFFFF repeat top /100%;color:#777777\">\n"
"    <p>\n"
"        Chèr(e) <t t-out=\"object.member_id.name.split()[0] or 'membre'\">membre</t>,\n"
"    </p>\n"
"    <p>\n"
"        Une demande d'affiliation a été faite pour la saison <u><i t-out=\"object.period_category_id.period_id.name or ''\">xxxx</i></u>.\n"
"        <br/>\n"
"        Pourriez-vous nous communiquer votre réponse en cliquant sur le bouton de votre choix?\n"
"    </p>\n"
"    <p>\n"
"        <a t-attf-href=\"/club/my/membership/accept?token={{ object.token }}\" style=\"padding: 5px 10px; font-size: 12px; line-height: 18px; color: #FFFFFF; border-color:#875A7B; text-decoration: none; display: inline-block; margin-bottom: 0px; font-weight: 400; text-align: center; vertical-align: middle; cursor: pointer; white-space: nowrap; background-image: none; background-color: #875A7B; border: 1px solid #875A7B; border-radius:3px\">Accepter</a>\n"
"        <a t-attf-href=\"/club/my/membership/decline?token={{ object.token }}\" style=\"padding: 5px 10px; font-size: 12px; line-height: 18px; color: #FFFFFF; border-color:#875A7B; text-decoration: none; display: inline-block; margin-bottom: 0px; font-weight: 400; text-align: center; vertical-align: middle; cursor: pointer; white-space: nowrap; background-image: none; background-color: #875A7B; border: 1px solid #875A7B; border-radius:3px\">Refuser</a>\n"
"        <a t-attf-href=\"/club/my/membership/view?token={{ object.token }}\" style=\"padding: 5px 10px; font-size: 12px; line-height: 18px; color: #FFFFFF; border-color:#875A7B; text-decoration: none; display: inline-block; margin-bottom: 0px; font-weight: 400; text-align: center; vertical-align: middle; cursor: pointer; white-space: nowrap; background-image: none; background-color: #875A7B; border: 1px solid #875A7B; border-radius:3px\">Voir</a>\n"
"    </p>\n"
"    <p>\n"
"        Bien à vous,\n"
"    </p>\n"
"    <p>\n"
"        Les membres du comité du <t t-out=\"user.company_id.name or ''\">YourCompany</t>\n"
"    </p>\n"
"    <img src=\"/logo.png\" style=\"padding: 0px; margin: 0px; height: auto; width: 80px;\" alt=\"{{ user.company_id.name }}\"/>\n"
"\n"
"</div>\n"
"        "

