#: model:mail.template,body_html:sport_club_manager.email_template_membership_affiliation_request
msgid ""
"\n"
"<div summary=\"o_mail_template\" style=\"padding:0px;width:600px;margin:auto;"
"background: #FFFFFF repeat top /100%;color:#777777\">\n"
"    <p>\n"
"        Dear ${(object.user_id.name or 'player')|safe},\n"
"    </p>\n"
"    <p>\n"
"        An affiliation request has been done for the season <u><i>${object."
"period_category_id.period_id.name|safe}</i></u>.\n"
"        <br/>\n"
"        Could you please let us know your answer by clicking on the button "
"of your choice?\n"
"    </p>\n"
"    <p>\n"
"        <a href=\"/sport/my/membership/accept?db=${'dbname' in ctx and "
"ctx['dbname'] or ''}&token=${object.token}&action=${'action_id' in ctx and "
"ctx['action_id'] or ''}&id=${object.id}\" style=\"padding: 5px 10px; font-"
"size: 12px; line-height: 18px; color: #FFFFFF; border-color:#875A7B; text-"
"decoration: none; display: inline-block; margin-bottom: 0px; font-weight: "
"400; text-align: center; vertical-align: middle; cursor: pointer; white-"
"space: nowrap; background-image: none; background-color: #875A7B; border: "
"1px solid #875A7B; border-radius:3px\">Accept</a>\n"
"        <a href=\"/sport/my/membership/decline?db=${'dbname' in ctx and "
"ctx['dbname'] or '' }&token=${object.token}&action=${'action_id' in ctx and "
"ctx['action_id'] or ''}&id=${object.id}\" style=\"padding: 5px 10px; font-"
"size: 12px; line-height: 18px; color: #FFFFFF; border-color:#875A7B; text-"
"decoration: none; display: inline-block; margin-bottom: 0px; font-weight: "
"400; text-align: center; vertical-align: middle; cursor: pointer; white-"
"space: nowrap; background-image: none; background-color: #875A7B; border: "
"1px solid #875A7B; border-radius:3px\">Decline</a>\n"
"        <a href=\"/sport/my/membership/view?db=${'dbname' in ctx and "
"ctx['dbname'] or ''}&token=${object.token}&action=${'action_id' in ctx and "
"ctx['action_id'] or ''}&id=${object.id}\" style=\"padding: 5px 10px; font-"
"size: 12px; line-height: 18px; color: #FFFFFF; border-color:#875A7B; text-"
"decoration: none; display: inline-block; margin-bottom: 0px; font-weight: "
"400; text-align: center; vertical-align: middle; cursor: pointer; white-"
